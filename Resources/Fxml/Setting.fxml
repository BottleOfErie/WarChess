<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ck.ckrc.erie.warchess.Controller.SettingController">
   <children>
      <Button fx:id="loadclass" layoutX="400.0" layoutY="100.0" mnemonicParsing="false" onMouseClicked="#LoadClass" prefHeight="50.0" prefWidth="100.0" text="load class" />
      <Button fx:id="decompile" layoutX="400.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#DeCompile" prefHeight="50.0" prefWidth="100.0" text="decompile" />
      <Button fx:id="back" layoutX="400.0" layoutY="400.0" mnemonicParsing="false" onMouseClicked="#GameStart" prefHeight="50.0" prefWidth="100.0" text="启动！" />
      <Button layoutX="400.0" layoutY="300.0" mnemonicParsing="false" onAction="#loadclassfromfile" prefHeight="50.0" prefWidth="100.0" text="file" />
      <ScrollPane id="notloadpane" layoutX="100.0" layoutY="50.0" prefHeight="500.0" prefWidth="200.0">
         <content>
            <VBox id="NotLoadClassList" fx:id="NotLoadClassList" prefHeight="500.0" prefWidth="200.0" style="-fx-border-color: BLACK;" />
         </content>
      </ScrollPane>
    <AnchorPane layoutX="481.0" layoutY="79.0" minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
      <ScrollPane id="loadedpane" layoutX="600.0" layoutY="50.0" prefHeight="500.0" prefWidth="200.0">
         <content>
            <VBox id="LoadedClassList" fx:id="LoadedClassList" prefHeight="500.0" prefWidth="200.0" style="-fx-border-color: BLACK;" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
